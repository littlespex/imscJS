        "filesaver.js-npm": "latest",
        "globals": "^15.0.0",
        "grunt": "latest",
        "grunt-contrib-clean": "latest",
        "grunt-exec": "^3.0.0",
        "grunt-npmcopy": "latest",
        "grunt-sync": "latest",
        "jszip": "latest",
        "node-qunit": "^1.0.0",
        "qunit-assert-close": "latest",
        "qunitjs": "latest",
        "rollup": "^4.14.3",
