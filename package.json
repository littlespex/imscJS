    "type": "module",
    "files": [
        "dist"
    ],
    "keywords": [
        "imsc1",
        "imsc1.0.1",
        "imsc",
        "imsc1.1",
        "ebut-tt-d",
        "smpte-tt",
        "sdp-us",
        "cff-tt",
        "ttml",
        "imsc"
    ],
    "author": "Pierre-Anthony Lemieux <pal@sandflow.com>",
